/*------TABLA-----*/
[objectName*=&quot;tabla_&quot;] {
    font-family: Arial;
    font-size: 14px;
background: #F5F5F5;
}

/* Encabezados de la tabla */
[objectName*=&quot;tabla_&quot;] QHeaderView::section {
    background-color: #023375;
    color: white;
    font-family: &quot;Arial&quot;;
    font-size: 16px;
    padding: 5px;
    border: 1px solid gray;
}

/* Encabezado horizontal */
[objectName*=&quot;tabla_&quot;] QHeaderView::horizontal {
    background-color: #023375;
    color: white;
}

/* Encabezado vertical */
[objectName*=&quot;tabla_&quot;] QHeaderView::vertical {
    background-color: #023375;
    color: white;
}


[objectName*=&quot;fecha_&quot;]{
border: none;
border-bottom: 1px solid #3b3c3d;
border-radius: 5px;
padding: 2px;
background-color: #f5f4f1;
font-size: 14px;
min-width:100px;
}
*[objectName*=&quot;fecha_&quot;]::drop-down{
subcontrol-origin: padding;
subcontrol-position: top right;
padding-right:5px;
width: 20%;
border-left-width: 1px;
borde-radius: 5px;
background-color: #F5F5F5;
}
*[objectName*=&quot;fecha_&quot;]::down-arrow{
image: url(:/Icons/Bootstrap/calendar2-date.svg);
width: 17%;
height: 17%;
padding-left:5px;
padding-right:5px;
background: #F5F5F5;
}
*[objectName*=&quot;fecha_&quot;]::focus{
border-bottom: 2px solid #00668c;
}
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: #00668c;
    color: white;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}

/* BOTONES DE NAVEGACIÓN */
QCalendarWidget QToolButton {
    background-color: transparent;
    color: white;
    font-weight: bold;
    padding: 5px;
}

/* DESPLEGABLE DE MESES */
QCalendarWidget QMenu {
    background-color: #FFFFFF;
    border: 1px solid #3b3c3d;
}

/* DÍAS DE LA SEMANA */
QCalendarWidget QWidget#qt_calendar_weekdaybar {
    background-color: #f5f4f1;
    color: #3b3c3d;
}

/* DÍAS DEL MES */
QCalendarWidget QAbstractItemView:enabled {
    background-color: #FFFFFF;
    color: #1d1c1c;
    selection-background-color: #00668c;  /* Color al seleccionar fecha */
    selection-color: white;
}

/* DÍAS DE OTROS MESES */
QCalendarWidget QAbstractItemView:disabled {
    color: #AAAAAA;
}

/* EFECTO HOVER SOBRE DÍAS */
QCalendarWidget QAbstractItemView::item:hover {
    background-color: #e1f0f7;
    color: #1d1c1c;
}

